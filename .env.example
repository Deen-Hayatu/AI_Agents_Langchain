# Environment Variables for AI Agents Course
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (required for most examples)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (optional, for Claude examples)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Tavily API Key (for web search examples)
TAVILY_API_KEY=your_tavily_api_key_here

# Alpha Vantage API Key (for financial examples)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Weather API Key (for weather tool examples)
WEATHER_API_KEY=your_weather_api_key_here

# Other optional APIs
SERPAPI_API_KEY=your_serpapi_key_here
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=ai_agents_course
